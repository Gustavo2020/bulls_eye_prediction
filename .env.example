# Copy this file to .env and fill in your values before running the pipeline.
# NEVER commit .env to version control.

# ntfy.sh topic URL for push notifications
# Create a free topic at https://ntfy.sh
NTFY_TOPIC=https://ntfy.sh/YOUR-NTFY-TOPIC

# Absolute path to the project directory
PROJECT_DIR=/home/YOUR_USERNAME/path/to/FINAL_PROJECT_VF

# Absolute path to the Python virtual environment (without /bin/activate)
VENV_DIR=/home/YOUR_USERNAME/path/to/tsenvi

# Streamlit port (must match .streamlit/config.toml and nginx config)
APP_PORT=8502
